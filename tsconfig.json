{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "baseUrl": ".",
    "lib": [
      "es2015",
      "es2017",
      "dom",
      "es5",
      "es6"
    ],
    "allowSyntheticDefaultImports": false,
    "allowJs": false,
    "paths": {
